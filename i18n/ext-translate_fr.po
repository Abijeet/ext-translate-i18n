msgid "Log for actions related to the page translation system"
msgstr "Journal des actions liées au système de traduction de pages"

#. [Wiki] {{doc-logpage}}
msgctxt "log-name-pagetranslation"
msgid "Page translation log"
msgstr "Journal des traductions de pages"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
msgctxt "logentry-pagetranslation-mark"
msgid "$1 {{GENDER:$2|marked}} $3 for translation"
msgstr "$1{{GENDER:$2|}} a marqué $3 à traduire"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
msgctxt "logentry-pagetranslation-unmark"
msgid "$1 {{GENDER:$2|removed}} $3 from the translation system"
msgstr "$1{{GENDER:$2|}} a retiré $3 du système de traduction"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
msgctxt "logentry-pagetranslation-moveok"
msgid "$1 {{GENDER:$2|completed}} renaming of translatable page $3 to $4"
msgstr "$1{{GENDER:$2|}} a terminé le renommage de la page traduisible $3 en $4"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
msgctxt "logentry-pagetranslation-movenok"
msgid "$1 {{GENDER:$2|encountered}} a problem while moving page $3 to $4"
msgstr "$1{{GENDER:$2|}} a rencontré un problème en déplaçant la page $3 vers $4"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
msgctxt "logentry-pagetranslation-deletefok"
msgid "$1 {{GENDER:$2|completed}} deletion of translatable page $3"
msgstr "$1{{GENDER:$2|}} a terminé la suppression de la page traduisible $3"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
msgctxt "logentry-pagetranslation-deletefnok"
msgid "$1 {{GENDER:$2|failed}} to delete $3 which belongs to translatable page $4"
msgstr "$1{{GENDER:$2|}} n’a pas réussi à supprimer $3 qui appartient à la page traduisible $4"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
msgctxt "logentry-pagetranslation-deletelok"
msgid "$1 {{GENDER:$2|completed}} deletion of translation page $3"
msgstr "$1{{GENDER:$2|}} a terminé la suppression de la page de traduction $3"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
msgctxt "logentry-pagetranslation-deletelnok"
msgid "$1 {{GENDER:$2|failed}} to delete $3 which belongs to translation page $4"
msgstr "$1{{GENDER:$2|}} n’a pas réussi à supprimer $3 qui appartient à la page de traduction $4"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
msgctxt "logentry-pagetranslation-encourage"
msgid "$1 {{GENDER:$2|encouraged}} translation of $3"
msgstr "$1{{GENDER:$2|}} a incité à traduire $3"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
msgctxt "logentry-pagetranslation-discourage"
msgid "$1 {{GENDER:$2|discouraged}} translation of $3"
msgstr "$1{{GENDER:$2|}} a dissuadé la traduction de $3"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
msgctxt "logentry-pagetranslation-prioritylanguages-unset"
msgid "$1 {{GENDER:$2|removed}} priority languages from translatable page $3"
msgstr "$1{{GENDER:$2|}} a supprimé les langues prioritaires pour la page traduisible $3"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
#. [Wiki] * $5 is list of languages (A, B and C)
msgctxt "logentry-pagetranslation-prioritylanguages"
msgid "$1 {{GENDER:$2|set}} the priority languages for translatable page $3 to $5"
msgstr "$1{{GENDER:$2|}} a défini les langues prioritaires pour la page traduisible $3 à $5"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
#. [Wiki] * $5 is list of languages (A, B and C)
msgctxt "logentry-pagetranslation-prioritylanguages-force"
msgid "$1 {{GENDER:$2|limited}} languages for translatable page $3 to $5"
msgstr "$1{{GENDER:$2|}} a limité les langues pour la page traduisible $3 à $5"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
#. [Wiki] * $4 is the name of the aggregate group
msgctxt "logentry-pagetranslation-associate"
msgid "$1 {{GENDER:$2|added}} translatable page $3 to aggregate group $4"
msgstr "$1{{GENDER:$2|}} a ajouté la page traduisible $3 au groupe agrégé $4"

#. [Wiki] {{logentry|[[Special:Log/pagetranslation]]}}
#. [Wiki] * $4 is the name of the aggregate group
msgctxt "logentry-pagetranslation-dissociate"
msgid "$1 {{GENDER:$2|removed}} translatable page $3 from aggregate group $4"
msgstr "$1{{GENDER:$2|}} a retiré la page traduisible $3 du groupe agrégé $4"

